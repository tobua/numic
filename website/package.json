    "react-dom": "^18.2.0",
    "three": "^0.159.0",
    "typescript": "^5.3.3",
    "valtio": "^1.12.1"
  },
  "type": "module",
  "prettier": "papua/configuration/.prettierrc.json",
  "eslintConfig": {
    "plugins": [
