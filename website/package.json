{
  "$schema": "https://raw.githubusercontent.com/tobua/papua/main/schema.json",
  "name": "numic",
  "scripts": {
    "start": "papua start"
  },
  "papua": {
    "title": "numic for React Native",
    "icon": "../logo.png"
  },
  "dependencies": {
    "@react-three/drei": "^9.88.0",
    "@react-three/eslint-plugin": "^0.1.1",
    "@react-three/fiber": "^8.14.5",
    "@react-three/postprocessing": "^2.15.1",
    "@react-three/rapier": "^1.1.1",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/three": "^0.157.0",
    "maath": "^0.10.4",
    "papua": "^5.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.157.0",
    "typescript": "^5.2.2",
    "valtio": "^1.11.2"
  },
  "type": "module",
  "prettier": "papua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": [
      "./node_modules/papua/configuration/eslint.cjs",
      "plugin:@react-three/recommended"
    ],
    "plugins": [
